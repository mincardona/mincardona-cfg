# BEGIN SECTION custom

bind '"\t":menu-complete'

mkcd() {
    mkdir $1;
    cd $1;
}

cdls() {
    cd $1;
    echo $(ls);
}

cdla() {
    cd $1;
    echo "$(ls -al)";
}

alias temacs='emacs -nw'

alias ping4='ping -c 4'

math() {
    echo "$1" | bc -l;
}

